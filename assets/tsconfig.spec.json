{
  "include": [
    "ts/**/*"
  ],
  "compilerOptions": {
    "typeRoots": ["../node_modules/@types"],
    "types": [
      "leaflet",
      "geojson",
      "jest",
      "node"
    ],
    "outDir": "./dist/",
    "rootDir": "./ts/",
    "noImplicitAny": true,
    "allowJs": true,
    "module": "es6",
    "target": "es2015",
    "moduleResolution": "node",
    "lib": ["esnext", "dom"]
  }
}
